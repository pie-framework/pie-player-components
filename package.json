{
  "name": "@pie-framework/pie-player-components",
  "version": "0.5.3",
  "description": "Pie Player Components",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "unpkg": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --es5 --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "release": "npm run build && release-it --no-git.requireCleanWorkingDir",
    "demo": "npm run start & http-server www",
    "yalc": "yalc push"
  },
  "dependencies": {
    "@material/select": "^2.2.0",
    "@material/switch": "^2.1.1",
    "@material/tab": "^2.2.0",
    "@material/tab-bar": "^2.2.0",
    "@pie-framework/pie-configure-events": "^1.2.0",
    "@stencil/sass": "^0.2.1",
    "async-retry": "^1.2.3",
    "parse-package-name": "^0.1.0"
  },
  "devDependencies": {
    "@pie-framework/pie-player-events": "^0.1.0",
    "@release-it/conventional-changelog": "^1.0.0",
    "@stencil/core": "~0.18.0",
    "@types/jest": "23.3.14",
    "@types/lodash": "^4.14.121",
    "@types/puppeteer": "1.12.1",
    "http-server": "^0.11.1",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "jest-environment-node-debug": "^2.0.0",
    "lodash": "^4.17.11",
    "prettier": "1.16.4",
    "puppeteer": "1.11.0",
    "release-it": "^10.1.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3",
    "yalc": "^1.0.0-pre.27"
  },
  "license": "MIT",
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "angular",
      "infile": "CHANGELOG.md"
    }
  }
}
